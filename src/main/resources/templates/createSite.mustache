    <form id="myForm" >
        <input id="userSeq" type="text" name="userSeq" value="{{#userSeq}}{{userSeq}}{{/userSeq}}" hidden/>
       <input id="siteUrl" type="text" name="siteUrl" placeholder="site url" />
        <input id="title" type="text" name="title" placeholder="title" />
       <button id="btn-save" type="button"> Save</button>

    </form>



</div>




<script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
<script>
    $('#btn-save').on('click', saveSite);
    function saveSite() {

        // var formData = JSON.stringify($("#myForm").serializeArray());

        $.ajax({
            type: "POST",
            url: "/sites",
            async: true,
            // data: formData,
            processData: false,
            contentType: false,
            data: {
                siteUrl: $("#siteUrl").val(),
                userSeq: $("#userSeq").val(),
                title: $("#title").val(),
            },
            success: function(){
                console.log("success");
            },

            // dataType: "json",
            contentType : "application/json"
        }).fail(function (error) {
            console.warn(error);
        });
    }

</script>